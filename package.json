{
  "name": "fairpost",
  "version": "2.1.0",
  "description": "Feeds data to social media platforms based on folders in a dir",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "test": "npm run lint",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
