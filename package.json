    "google-auth-library": "^9.4.2",
    "log4js": "^6.9.1",
    "node-fetch": "^2.6.7",
    "sharp": "0.33.1",
    "twitter-api-v2": "^1.15.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-prettier": "^5.0.1",
